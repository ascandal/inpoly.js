{
    "name": "@ascandal/inpoly.js",
    "version": "1.0.0",
    "description": "A small JavaScript library for polygon spatial queries.",
    "keywords": [
        "polygon",
        "rectangle",
        "geometry",
        "points"
    ],
    "author": "Angelo L. Scandaliato",
    "license": "MIT",
    "repository": "https://github.com/ascandal/inpoly.js",
    "bugs": "https://github.com/ascandal/inpoly.js/issues",
    "homepage": "https://github.com/ascandal/inpoly.js#readme",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs",
            "default": "./dist/index.mjs"
        },
        "./react": {
            "types": "./dist/react/index.d.ts",
            "import": "./dist/react/index.mjs",
            "require": "./dist/react/index.cjs",
            "default": "./dist/react/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "restricted"
    },
    "engines": {
        "node": ">=20"
    },
    "sideEffects": false,
    "scripts": {
        "build": "rm -rf dist && tsdown --clean",
        "build:watch": "tsdown --watch",
        "build:meta": "tsdown --metafile --silent",
        "pack": "npm run build && npm pack",
        "typecheck": "tsc --noEmit",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "lint:fix-dry-run": "eslint . --fix-dry-run",
        "check-format": "prettier --check .",
        "format": "prettier --write .",
        "serve:html": "npx http-server -c-1 -p 8080",
        "serve:dev": "vite dev",
        "preview": "vite preview",
        "test": "node --test",
        "prepublishOnly:all": "npm run typecheck && npm run lint && npm run build",
        "ci": "npm run build && npm run check-format && npm run test"
    },
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@playwright/test": "^1.56.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@types/node": "^24.10.0",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@typescript-eslint/eslint-plugin": "^8.40.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^16.5.0",
        "playwright": "^1.52.0",
        "prettier": "^3.6.2",
        "terser": "^5.44.1",
        "tsdown": "^0.15.12",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.3",
        "vite": "^7.2.0"
    },
    "peerDependencies": {
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    },
    "dependencies": {
        "static-kdtree": "^1.0.2"
    }
}
